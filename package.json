{
  "name": "optimize-abtest",
  "version": "1.0.7",
  "description": "Adds integrity of Google Optimize into your React app",
  "keywords": [
    "google optimize",
    "optimize",
    "optimize-abtest",
    "ab test",
    "ab experiment",
    "react"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/index.ts\" \"dist/index.js\"",
    "eslint": "export NODE_OPTIONS=--max_old_space_size=8192;eslint src/**/* --ext ts,tsx --cache --cache-location '.cache/.eslintcache/' --quiet",
    "prepare": "npm run build && npm run eslint && npm run format",
    "release": "npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkireev/abtest.git"
  },
  "author": "Dmytro Kireiev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dkireev/abtest/issues"
  },
  "homepage": "https://github.com/dkireev/abtest#readme",
  "devDependencies": {
    "@types/js-cookie": "^2.2.6",
    "@types/react": "^17.0.13",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.29.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "files": [
    "dist/**.*"
  ]
}
