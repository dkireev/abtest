{
  "name": "optimize-abtest",
  "version": "1.0.5",
  "description": "Adds integrity of Google Optimize into your React app",
  "keywords": [
    "google optimize",
    "optimize",
    "optimize-abtest",
    "ab test",
    "ab experiment",
    "react"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/index.ts\" \"dist/index.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build && npm run lint && npm run format",
    "release": "npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkireev/abtest.git"
  },
  "author": "Dmytro Kireiev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dkireev/abtest/issues"
  },
  "homepage": "https://github.com/dkireev/abtest#readme",
  "devDependencies": {
    "@types/js-cookie": "^2.2.6",
    "@types/react": "^17.0.3",
    "prettier": "^2.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  },
  "files": [
    "dist/**.*"
  ]
}
